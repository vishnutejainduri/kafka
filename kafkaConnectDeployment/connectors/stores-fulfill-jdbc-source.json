{
  "name": "stores-fulfill-jdbc-source",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSourceConnector",
    "tasks.max": 1,
    "connection.url": "jdbc:oracle:thin:<DB_USERNAME>/<DB_PASSWORD>@//<DB_HOST_IP>:1521/<DB_SERVICE_NAME>",
    "schema.pattern": "ELCAT",
    "table.poll.interval.ms": 3600000,
    "mode": "timestamp",
    "timestamp.column.name": "LAST_MODIFIED",
    "query": "SELECT STORE_CD AS SITE_ID, FULLFILL AS FULFILL_STATUS, LAST_MODIFIED FROM ELCAT.STOREFULLFILL",
    "topic.prefix": "stores-fulfill-connect-jdbc",
    "validate.non.null": "false",
    "numeric.mapping": "best_fit",
    "poll.interval.ms": 120000,
    "offset.flush.timeout.ms": 60000
  }
}
