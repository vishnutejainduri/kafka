{
  "name": "shipments-jdbc-source",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSourceConnector",
    "tasks.max": 1,
    "connection.url": "jdbc:oracle:thin:<DB_USERNAME>/<DB_PASSWORD>@//<DB_HOST_IP>:1521/<DB_SERVICE_NAME>",
    "schema.pattern": "E_SHIPMENTS",
    "timestamp.column.name": "MODIFIED_DATE",
    "table.poll.interval.ms": 3600000,
    "mode": "timestamp",
    "query": "SELECT es.WFE_TRANS_ID AS ORDER_NUMBER, es.SHIPMENT_ID, es.SERVICE_TYPE, es.DEST_SITE_ID, es.MODIFIED_DATE FROM EDOM.E_SHIPMENTS es",
    "topic.prefix": "shipments-connect-jdbc",
    "validate.non.null": "false",
    "numeric.mapping": "best_fit",
    "poll.interval.ms": 60000, 
    "offset.flush.timeout.ms": 3600000
  }
}
