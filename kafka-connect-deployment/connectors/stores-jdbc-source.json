{
  "name": "stores-jdbc-source",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSourceConnector",
    "tasks.max": 1,
    "connection.url": "jdbc:oracle:thin:<DB_USERNAME>/<DB_PASSWORD>t@//<DB_HOST_IP>:1521/<DB_SERVICE_NAME>",
    "schema.pattern": "MERCH",
    "table.whitelist": "SITES",
    "table.poll.interval.ms": 3600000,
    "mode": "bulk",
    "topic.prefix": "stores-connect-jdbc-",
    "validate.non.null": "false",
    "numeric.mapping": "best_fit",
    "poll.interval.ms": 43200000,
    "offset.flush.timeout.ms": 60000
  }
}